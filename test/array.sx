test (a) {
	foreach (i in a) {
		printl ('--', i);
		for (n = 3; n <= 9; n += 3) {
			printl ('In - ', n, ':', i);
		}
	}
}

main () {
	a = ['abc','def','ghi'];

	printl(a);

	test(a);

	if ('def' in a) {
		printl ('Correct - def in array');
	} else {
		printl ('Incorrect - def !in array');
	}

	if ('foo' in a) {
		printl ('Incorrect - foo in array');
	} else {
		printl ('Correct - foo !in array');
	}
}
