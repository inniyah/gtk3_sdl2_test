class test
	function set_name (name)
		self.name = name
	end
	function get_name ()
		self.name
	end
end

class child : test
	function set_name (name)
		self.name = concat ('child ', name)
	end
end

a = new test
b = new child

if a == b then
	printl ('== test failed')
	return 1
end

a.set_name ('test')
b.set_name ('test')

if b.get_name () != 'child test' or a.get_name () != 'test' then
	printl ('get_name/set_name failed')
	return 1
end

if ! a isa test then
	printl ('a isa failed')
end

if ! b isa test then
	printl ('b isa test failed')
end

if ! b isa child then
	printl ('b isa child failed')
end

printl ('success')
